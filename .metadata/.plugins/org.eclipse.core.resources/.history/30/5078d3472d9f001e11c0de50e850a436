package session5;

public class DemoForLoop {

	public static void main(String[] args) {
		//Vòng lặp in các số
		for(int i = 0; i < 5; i++) {
			 System.out.println(i + " " );
		}
		
		// Câu lệnh tiếp theo
		int sum = 0;
		for (int i = 1; i <= 10; i++) {
		 sum += i;
		}
		System.out.println("Sum = " + sum);

	}

}
