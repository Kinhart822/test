package session5;

public class DemoForLoop {

	public static void main(String[] args) {
		//Vòng lặp in các số
		for(int i = 0; i < 5; i++) {
			 System.out.print(i + " " );
		}
		
		// Câu lệnh tiếp theo
		System.out.println();	
		int sum = 0;
		for (int i = 1; i <= 1000; i++) {
			sum += i;
			System.out.print(i + " " );
			System.out.println("Sum = " + sum);
		}
		

	}

}
